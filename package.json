{
  "name": "ExaltedSkins",
  "version": "2.0.1",
  "description": "",
  "type": "commonjs",
  "main": "build/main.js",
  "scripts": {
    "dev": "electron src/",
    "start": "npm run compile && electron build/main.js",
    "compile": "tsc",
    "dist": " tsc && cd exalted_skins_react && npm run build && cd .. && electron-builder "
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "livekit-client": "^2.11.3",
    "livekit-server-sdk": "^2.12.0",
    "simple-peer": "^9.11.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "copyfiles": "^2.4.1",
    "electron": "^35.1.4",
    "electron-builder": "^26.0.12",
    "prisma": "^6.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "build": {
    "extraResources": [
      {
        "from": "./.env",
        "to": "./.env"
      }
    ],
    "appId": "com.example.Master_Shifo_Skins",
    "productName": "Master Shifo Skins",
    "asarUnpack": [
      "db/ExaltedSkins.db",
      "src/cslol-tools/**/*",
      "src/config.ini"
    ],
    "files": [
      "build/**/*",
      "prisma/**/*",
      "src/cslol-tools/**/*",
      "src/config.ini",
      "db/ExaltedSkins.db"
    ],
    "directories": {
      "output": "dist"
    },
    "asar": true
  },
  "win": {
    "icon": "icon.ico",
    "target": "nsis"
  },
  "nsis": {},
  "private": true
}
