{
  "name": "ExaltedSkins",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "build/main.js",
  "scripts": {
    "dev": "electron src/",
    "start": "npm run compile && electron build/main.js",
    "compile": "tsc",
    "dist": " tsc && cd exalted_skins_react && npm run build && cd .. && electron-builder "
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "axios": "^1.8.4",
    "electron": "^35.1.4"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "copyfiles": "^2.4.1",
    "electron-builder": "^26.0.12",
    "prisma": "^6.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  },
  "build": {
    "appId": "com.example.ExaltedSkinsMS",
    "productName": "Exalted Skins By M.S",
    "asarUnpack": [
      "db/ExaltedSkins.db",
      "src/cslol-tools/**/*",
      "src/config.ini",
      "depend/Git-2.45.2-64-bit"
    ],
    "files": [
      "build/**/*",
      "prisma/**/*",
      "src/cslol-tools/**/*",
      "src/config.ini",
      "db/ExaltedSkins.db",
      "depend/Git-2.45.2-64-bit"
    ],
    "directories": {
      "output": "dist"
    },
    "asar": true
  },
  "win": {
    "icon": "icon.ico",
    "target": "nsis"
  },
  "nsis": {},
  "private": true
}
